function ip(){$if=json_decode(file_get_contents("https://ipinfo.io/?utm_source=ipecho.net&utm_medium=referral&utm_campaign=upsell_sister_sites"));return ["i"=>$if->ip,"r"=>$if->region,"c"=>$if->country,"t"=>$if->timezone];}

function captchaai($method,$apikey,$sitekey,$pageurl){$recaptchav2=http_build_query(["key"=>$apikey,"method"=>"userrecaptcha","googlekey"=>$sitekey,"pageurl"=>$pageurl]);$hcaptcha=http_build_query(["key"=>$apikey,"method"=>"hcaptcha","sitekey"=>$sitekey,"pageurl"=>$pageurl]);$type=["hcaptcha"=>$hcaptcha,"recaptchav2"=>$recaptchav2];$ua=["host: ocr.captchaai.com","content-type: application/json/x-www-form-urlencoded"];while(true){$n=[15,20];$t=$n[array_rand($n)];$r=curl("http://ocr.captchaai.com/in.php?".$type[$method],$ua)[1];
$id=explode('|',$r)[1];if(!$id){sleep($t);continue;}sleep($t);while(true){print "prosess...";
$r=curl("http://ocr.captchaai.com/res.php?".http_build_query(["key"=>$apikey,"action"=>"get","id"=>$id]),$ua)[1]
;if($r=="CAPCHA_NOT_READY"){r();print "prosess......";
sleep(10);r();continue;}r();return explode('|', $r)[1];}}}

function HcaptchaAny($siteurl,$sitekey,$apikey){$h=["Host: api.anycaptcha.com","Content-Type: application/json"];$data=json_encode(["clientKey"=>$apikey]);$r=json_decode(curl("https://api.anycaptcha.com/getBalance",$h,$data)[1],1);if($r["balance"]<=0){exit("Balance Apikey Habis");}$data=json_encode(["clientKey"=>$apikey,"task"=>["type"=>"HCaptchaTaskProxyless","websiteURL"=>$siteurl,"websiteKey"=>$sitekey]]);$Create=json_decode(curl('https://api.anycaptcha.com/createTask',$h,$data)[1]);if($Create->errorId>0){exit(m.$Create->errorCode.n);}else{while(true){$base=json_encode(["clientKey"=>$apikey,"taskId"=>$Create->taskId]);$Result=json_decode(curl('https://api.anycaptcha.com/getTaskResult',$h,$base)[1]);if($Result->status=='processing'){print k.$Result->status;sleep(3);r();continue;}r();return $Result->solution->gRecaptchaResponse;}}}

function RecaptchaV2Any($siteurl,$sitekey,$apikey){$h=["Host: api.anycaptcha.com","Content-Type: application/json"];$data = json_encode(["clientKey"=>$apikey]);$r = json_decode(curl("https://api.anycaptcha.com/getBalance",$h,$data)[1],1);if($r["balance"] <= 0){exit("Balance Apikey Habis");}$data=json_encode(["clientKey"=>$apikey,"task"=>["type"=>"RecaptchaV2TaskProxyless","websiteURL"=>$siteurl,"websiteKey"=>$sitekey,"isInvisible"=>false]]);$Create=json_decode(curl('https://api.anycaptcha.com/createTask',$h,$data)[1]);if($Create->errorId > 0){exit(m.$Create->errorCode.n);}else{while(true){print k."Prosessing...";$base=json_encode(["clientKey"=>$apikey,"taskId"=>$Create->taskId]);$Result=json_decode(curl('https://api.anycaptcha.com/getTaskResult',$h,$base)[1]);if($Result->status=='processing'){print k.$Result->status;sleep(3);r();continue;}r();return $Result->solution->gRecaptchaResponse;}}}

function reload_recaptchav3($grecaptcha){$render=curl("https://www.google.com/recaptcha/api.js?".http_build_query(["render"=>$grecaptcha]))[1];preg_match('#/releases/(.*?)/#is',$render,$v);if($v[1]){return $v[1];}}

function recaptchav3($grecaptcha,$v,$referer){
$cookie=cookie_recaptchav3();
$google="https://www.google.com/recaptcha/";
$hash=str_replace("=",".",base64_encode("https://".explode("/",host)[2].":443"));
$country=["id","en"][1];
$url_anchor=$google."api2/anchor?".http_build_query(["ar"=>1,"k"=>$grecaptcha,"co"=>$hash,"hl"=>$country,"v"=>$v,"size"=>"invisible","cb"=>substr(str_shuffle("0123456789abcdefghijklmnopqrstuvwxyz"),0,12)]);
$anchor=curl($url_anchor,h_google($referer,$cookie))[1];
preg_match('#id="recaptcha-token" value="(.*?)"#is',$anchor,$token);
$data=http_build_query(["v"=>$v,"reason"=>"q","c"=>$token[1],"k"=>$grecaptcha,"co"=>$hash,"hl"=>$country,"size"=>"invisible","chr"=>"[".rand(1,99).",".rand(1,99).",".rand(1,99)."]","vh"=>"1".rand(1111111111,9999999999)]);
//die($data);
$reload=curl($google."api2/reload?".http_build_query(["k"=>$grecaptcha]),h_google($url_anchor,$cookie,$data),$data)[1];
preg_match('#rresp","(.*?)"#is',$reload,$results);
if($results[1]){return $results[1];}}

