function h_google($referer,$cookie,$data=0){
$h[]="Host: www.google.com";
$h[]="user-agent: Mozilla/5.0 (Linux; Android 11; M2012K11AG) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/105.0.0.0 Mobile Safari/537.36";
if($data){
$h[]="content-length: ".strlen($data);
$h[]="accept: */*";
$h[]="origin: https://www.google.com";
}else{
$h[]="upgrade-insecure-requests: 1";
$h[]="accept: text/html,application/xhtml+xml,application/xml;q=0.9,image/avif,image/webp,image/apng,*/*;q=0.8,application/signed-exchange;v=b3;q=0.9";}
$h[]="referer: ".$referer;
$h[]="accept-language: id-ID,id;q=0.9,en-US;q=0.8,en;q=0.7";
$h[]="cookie: ".$cookie;
return $h;}

