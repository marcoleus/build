function antb($ab){
$a=$ab[1][0];$b=$ab[1][1];$c=$ab[1][2];
return [[" ".$b,$c,$a],[" ".$a,$b,$c],[" ".$a,$c,$b],[" ".$b,$a,$c],[" ".$c,$b,$a],[" ".$c,$a,$b]];}

